<div class="box-table">
    <div class="table">
        <% head = {
            ni: "Nº invent.",
            instituicao: "Instituição" ,
            tag: "Tag" ,
            descricao: "Descrição" ,
            marca: "Marca",
            serie: "Série",
            sala: "Sala" ,
            localizacao: "Local" ,
            ativo: "Ativo" ,
        } %>

        <% if (maintenance) { %>
            <% head.manutencao = "Manutenção" %>
        <% } %>

        <% if (userType == "adm") { %>
            <% if (update) { %>
                <% head.atualizacao = "Atualização" %>
            <% } %>
        <% } %>

        <% if (historic) { %>
            <% head.historico = "Histórico" %>
        <% } %>

        <%- include('./tabela', {
            head: head,

            tabela: tabela["data"]
        }) %>
    </div>
</div>